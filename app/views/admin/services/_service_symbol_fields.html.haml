.clearfix
	= image_tag(resource.service_symbol.asset_path_url(:thumb)) if resource.service_symbol && resource.service_symbol.asset_path?
	= f.input :asset_path, :label => false
	= f.input :asset_path_cache, :as => :hidden
	- unless resource.new_record?
		= f.input :remove_asset_path, :label => "remove", :as => :boolean